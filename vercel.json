{
  "rewrites": [
    { "source": "/api/:match*", "destination": "/api/index.py" },

    { "source": "/favicon.ico", "destination": "/web/favicon.ico" },
    { "source": "/:file*.(css|js|png|jpg|jpeg|svg|webp)", "destination": "/web/:file*.:ext" },

    { "source": "/", "destination": "/web/index.html" },
    { "source": "/:path*", "destination": "/web/index.html" }
  ]
}
